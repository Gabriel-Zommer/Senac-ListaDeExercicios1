program divisao;

var
   x, t0, t1, t2, t3, t4, t5, t6, t7, t8, t9, t10: integer;

begin
   write('Digite o valor de x: ');
   readln(x);

   t0 := x * 0;
   t1 := x * 1;
   t2 := x * 2;
   t3 := x * 3;
   t4 := x * 4;
   t5 := x * 5;
   t6 := x * 6;
   t7 := x * 7;
   t8 := x * 8;
   t9 := x * 9;
   t10 := x * 10;

   writeln(x, '=', t0);
   writeln(x, '=', t1);
   writeln(x, '=', t2);
   writeln(x, '=', t3);
   writeln(x, '=', t4);
   writeln(x, '=', t5);
   writeln(x, '=', t6);
   writeln(x, '=', t7);
   writeln(x, '=', t8);
   writeln(x, '=', t9);
   writeln(x, '=', t10);
  
end.